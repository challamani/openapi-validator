{
  "openapi": "3.0.0",
  "info": {
    "title": "httpbin.org",
    "description": "A simple HTTP Request & Response Service.<br/> <br/> <b>Run locally: </b> <code>$ docker run -p 80:80 kennethreitz/httpbin</code>",
    "contact": {
      "name": "Kenneth Reitz",
      "email": "me@kennethreitz.org",
      "url": "https://kennethreitz.org"
    },
    "version": "0.9.2"
  },
  "servers": [
    {
      "url": "https://httpbin.org/",
      "description": "Main production server"
    }
  ],
  "paths": {
    "/get": {
      "get": {
        "tags": ["HTTP Methods"],
        "summary": "The request's query parameters.",
        "responses": {
          "200": {
            "description": "The request's query parameters.",
            "content": {
              "application/json": {
                "schema": { "type": "object" }
              }
            }
          }
        }
      }
    },
    "/anything": {
      "get": {
        "tags": ["Anything"],
        "summary": "Returns anything passed in request data.",
        "responses": {
          "200": {
            "description": "Anything passed in request",
            "content": {
              "application/json": {
                "schema": { "type": "object" }
              }
            }
          }
        }
      },
      "post": {
        "tags": ["Anything"],
        "summary": "Returns anything passed in request data.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": { "type": "object" }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Anything passed in request",
            "content": {
              "application/json": {
                "schema": { "type": "object" }
              }
            }
          }
        }
      }
    },
    "/bytes/{n}": {
      "get": {
        "tags": ["Dynamic data"],
        "summary": "Returns n random bytes generated with given seed",
        "parameters": [
          {
            "name": "n",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Bytes.",
            "content": {
              "application/octet-stream": {}
            }
          }
        }
      }
    },
    "/basic-auth/{user}/{passwd}": {
      "get": {
        "tags": ["Auth"],
        "summary": "Prompts the user for authorization using HTTP Basic Auth.",
        "parameters": [
          {
            "name": "user",
            "in": "path",
            "required": true,
            "schema": { "type": "string" }
          },
          {
            "name": "passwd",
            "in": "path",
            "required": true,
            "schema": { "type": "string" }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful authentication.",
            "content": {
              "application/json": {}
            }
          },
          "401": {
            "description": "Unsuccessful authentication."
          }
        }
      }
    },
    "/status/{codes}": {
      "get": {
        "tags": ["Status codes"],
        "summary": "Return status code or random status code if more than one are given",
        "parameters": [
          {
            "name": "codes",
            "in": "path",
            "required": true,
            "schema": { "type": "string" }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": { "text/plain": {} }
          },
          "400": { "description": "Client Errors" },
          "500": { "description": "Server Errors" }
        }
      }
    }
  },
  "components": {
    "schemas": {},
    "securitySchemes": {
      "basicAuth": {
        "type": "http",
        "scheme": "basic"
      },
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer"
      }
    }
  }
}